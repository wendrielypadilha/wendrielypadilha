    // Declara√ß√£o das vari√°veis
    int a = 10;
    int b = 20;
    double c;

    // C√°lculo de c
    c = (a + b) / 2.0; 
    c = c - 40;

    // Armazenar o resultado em um vetor
    double[] v = new double[4];
    v[3] = a + b + c;

       
    Console.WriteLine("O valor de v[3] √©: " + v[3]);
}

